<h1>Users Index Page</h1>
<p>Find me in app/views/users/index.html.erb</p>
<h3> Current User Emails in Database </h3>

<table>
    <tr>
       <th>Name</th>
       <th>Created At</th>
    </tr>

    <% @users.each do |user| %>
    <tr>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td>
    </tr>
    <% end %>
</table>
