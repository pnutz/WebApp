<h1>Listing currencies</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Description</th>
    </tr>
  </thead>

  <tbody>
    <% @currencies.each do |currency| %>
      <tr>
        <td><%= currency.code %></td>
        <td><%= currency.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>
